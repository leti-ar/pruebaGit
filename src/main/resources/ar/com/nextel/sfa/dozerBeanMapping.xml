<?xml version="1.0" encoding="UTF-8"?>

<mappings xmlns="http://dozer.sourceforge.net" xsi:schemaLocation="http://dozer.sourceforge.net
http://dozer.sourceforge.net/schema/beanmapping.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

<configuration>

</configuration>
	<mapping>
		<class-a>ar.com.nextel.sfa.client.dto.CuentaSearchDto
		</class-a>
		<class-b>
			ar.com.nextel.business.cuentas.search.businessUnits.CuentaSearchData
		</class-b>
		<field>
			<a>categoria.id</a>
			<b>idCategoria</b>
		</field>
		<field>
			<a>busquedaPredefinida.id</a>
			<b>tipoPredefinida</b>
		</field>
		<field>
			<a>grupoDocumentoId.code</a>
			<b>grupoDocumentoId</b>
		</field>
		<!--
			Según
			ar.com.nextel.business.cuentas.search.predicates.CuentaSearchRequestDocumentValidator
			que usa el Grupo Documento
		-->
		<!--
			Esto no está habilitado porque la UI actualmente siempre FIJA el tipo
			de documento a DNI, porque no admite no tener un elemento
			seleccionado dentro del combo. rgm
		-->
		<!--
			<field> <a>tipoDocumento.code</a> <b>grupoDocumentoId</b> </field>
		-->
	</mapping>

	<mapping>
		<class-a>ar.com.nextel.model.personas.beans.TipoDocumento
		</class-a>
		<class-b>ar.com.nextel.sfa.client.dto.TipoDocumentoDto
		</class-b>
		<field>
			<a>id</a>
			<b>code</b>
		</field>
	</mapping>

	<mapping>
		<class-a>ar.com.nextel.model.personas.beans.Sexo
		</class-a>
		<class-b>ar.com.nextel.sfa.client.dto.SexoDto
		</class-b>
		<field>
			<a>id</a>
			<b>code</b>
		</field>
	</mapping>

	<mapping>
		<class-a>ar.com.nextel.model.cuentas.beans.ClaseCuenta
		</class-a>
		<class-b>ar.com.nextel.sfa.client.dto.ClaseCuentaDto
		</class-b>
	</mapping>

	<mapping>
		<class-a>ar.com.nextel.model.cuentas.beans.CondicionCuenta
		</class-a>
		<class-b>ar.com.nextel.sfa.client.dto.CondicionCuentaDto
		</class-b>
	</mapping>

	<mapping>
		<class-a>
			ar.com.nextel.business.solicitudes.creation.request.SolicitudServicioRequest
		</class-a>
		<class-b>ar.com.nextel.sfa.client.dto.SolicitudServicioRequestDto
		</class-b>
		<field
			custom-converter-id="identifiableObjectToId">
			<a>grupoSolicitud</a>
			<b>idGrupoSolicitud</b>
		</field>
		<field
			custom-converter-id="identifiableObjectToId">
			<a>cuentaPotencial</a>
			<b>idCuentaPotencial</b>
		</field>
		<field
			custom-converter-id="identifiableObjectToId">
			<a>cuenta</a>
			<b>idCuenta</b>
		</field>
	</mapping>
	
	<mapping map-id="ssCerradaResult">
		<class-a>
			ar.com.nextel.model.solicitudes.beans.SolicitudServicio
		</class-a>
		<class-b>ar.com.nextel.sfa.client.dto.SolicitudServicioCerradaResultDto
		</class-b>
		<field>
			<a>idVantive</a>
			<b>idSolicitudServicio</b>
		</field>
	</mapping>
	
	<mapping>
		<class-a>
			ar.com.nextel.business.legacy.vantive.dto.EstadoSolicitudServicioCerradaDTO
		</class-a>
		<class-b>ar.com.nextel.sfa.client.dto.CambiosSolicitudServicioDto
		</class-b>
	</mapping>

	<mapping>
		<class-a>ar.com.nextel.model.solicitudes.beans.SolicitudServicio
		</class-a>
		<class-b>ar.com.nextel.sfa.client.dto.SolicitudServicioDto
		</class-b>
		<field>
			<a>montoCreditoFideliacion</a>
			<b>montoCreditoFidelizacion</b>
		</field>
		<field>
			<a>cuenta.estadoCreditoFidelizacion.monto</a>
			<b>montoDisponible</b>
		</field>
		<field custom-converter-id="enumToDto">
			<a>origen</a>
			<b>origen</b>
		</field>
		<field custom-converter-id="enumToDto">
			<a>tipoAnticipo</a>
			<b>tipoAnticipo</b>
		</field>	
		<field>
			<a>cuenta.estadoCreditoFidelizacion.vencimiento</a>
			<b>fechaVencimientoCreditoFidelizacion</b>
		</field>
	</mapping>

	<mapping map-id="cuentaSolicitud" wildcard="false">
		<class-a>ar.com.nextel.model.cuentas.beans.Cuenta
		</class-a>
		<class-b>ar.com.nextel.sfa.client.dto.CuentaDto
		</class-b>
		<field>
			<a>id</a>
			<b>id</b>
		</field>
	</mapping>

	<mapping>
		<class-a>ar.com.nextel.model.personas.beans.Domicilio</class-a>
		<class-b>ar.com.nextel.sfa.client.dto.DomiciliosCuentaDto</class-b>
		<field type="one-way">
			<!-- a map-get-method="isLockedBySolicitud">lockedBySolicitud</a-->
			<a>lockedBySolicitud</a>
			<b>locked</b>
		</field>
	</mapping>
	<mapping>
		<class-a>ar.com.nextel.model.personas.beans.TipoDomicilioAsociado</class-a>
		<class-b>ar.com.nextel.sfa.client.dto.TipoDomicilioAsociadoDto</class-b>
	</mapping>
	<mapping>
		<class-a>ar.com.nextel.model.cuentas.beans.Cargo</class-a>
		<class-b>ar.com.nextel.sfa.client.dto.CargoDto</class-b>
	</mapping>

	<mapping>
		<class-a>ar.com.nextel.model.cuentas.beans.Cuenta</class-a>
		<class-b>ar.com.nextel.sfa.client.dto.CuentaDto</class-b>

		<field custom-converter="ar.com.nextel.sfa.server.converter.DatosPagoConverter">
			<a>datosPago</a>
			<b>datosPago</b>
		</field>
	</mapping>
	
</mappings>
